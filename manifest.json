{
  "manifest_version": 2,

  "name": "Swaybien",
  "description": "Opens a random bookmark from one of your bookmark folders.",
  "version": "0.0.5",
  
  "options_page": "options.html",

  "icons": { "16": "images/icon16.png",
           "48": "images/icon48.png",
          "128": "images/icon128.png" },

  "permissions": [
    "bookmarks",
    "storage"
  ],
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  }
}
